os\r\n      </button>\r\n    </div>\r\n  );\r\n};\r\n",{"ruleId":"155","replacedBy":"156"},{"ruleId":"157","replacedBy":"158"},{"ruleId":"159","severity":1,"message":"160","line":3,"column":8,"nodeType":"161","messageId":"162","endLine":3,"endColumn":16},{"ruleId":"163","severity":1,"message":"164","line":23,"column":1,"nodeType":"165","endLine":86,"endColumn":3},{"ruleId":"163","severity":1,"message":"164","line":3,"column":1,"nodeType":"165","endLine":21,"endColumn":3},{"ruleId":"163","severity":1,"message":"164","line":4,"column":1,"nodeType":"165","endLine":15,"endColumn":3},{"ruleId":"163","severity":1,"message":"164","line":3,"column":1,"nodeType":"165","endLine":11,"endColumn":3},{"ruleId":"163","severity":1,"message":"164","line":3,"column":1,"nodeType":"165","endLine":9,"endColumn":3},{"ruleId":"163","severity":1,"message":"164","line":4,"column":1,"nodeType":"165","endLine":18,"endColumn":3},{"ruleId":"163","severity":1,"message":"166","line":1,"column":1,"nodeType":"165","endLine":11,"endColumn":3},{"ruleId":"163","severity":1,"message":"164","line":5,"column":1,"nodeType":"165","endLine":32,"endColumn":3},{"ruleId":"163","severity":1,"message":"166","line":1,"column":1,"nodeType":"165","endLine":5,"endColumn":3},{"ruleId":"163","severity":1,"message":"164","line":3,"column":1,"nodeType":"165","endLine":6,"endColumn":3},{"ruleId":"163","severity":1,"message":"164","line":4,"column":1,"nodeType":"165","endLine":24,"endColumn":3},{"ruleId":"163","severity":1,"message":"164","line":1,"column":1,"nodeType":"165","endLine":17,"endColumn":3},{"ruleId":"163","severity":1,"message":"164","line":4,"column":1,"nodeType":"165","endLine":11,"endColumn":3},{"ruleId":"163","severity":1,"message":"164","line":3,"column":1,"nodeType":"165","endLine":11,"endColumn":3},{"ruleId":"163","severity":1,"message":"164","line":4,"column":1,"nodeType":"165","endLine":26,"endColumn":3},{"ruleId":"163","severity":1,"message":"164","line":3,"column":1,"nodeType":"165","endLine":18,"e